= stylesheet_link_tag    "application", media: :all
-# Keeps the footer at the bottom of the page.
- @sticky_footer=true
- content_for :title, "Categories-Show"

# Shows a category
.categoryContainer
  %p#notice= notice

  %p
    %b Name:
    = @category.name

  = link_to 'Edit', edit_category_path(@category)
  \|
  = link_to 'Back', categories_path
